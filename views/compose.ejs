

<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="post">
    <label for="title">Title</label>
    <input type="text" name="Post_Title" id="title" autocomplete="off" placeholder="Single word only">
    <label for="info">Post</label>
    <textarea name="Post_Info" id="info" cols="30" rows="10"></textarea>
    <div><button type="submit">Publish</button></div>
</form>
<%- include("partials/footer") -%>